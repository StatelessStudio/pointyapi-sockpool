{
	"name": "pointyapi-sockpool",
	"version": "0.0.5",
	"description": "Websocket pool for PointyAPI",
	"main": "index.js",
	"scripts": {
		"test": "npm run build:spec && npm run coverage",
		"build": "tsc --p tsconfig.json",
		"build:spec": "tsc --p tsconfig.json && tsc --p tsconfig.spec.json",
		"postinstall": "tsc --p tsconfig.dist.json",
		"doc": "typedoc --out docs src --excludeNotExported",
		"coverage": "nyc npm run coverage-test",
		"coverage-test": "jasmine --config=test/jasmine.json",
		"coveralls": "cat ./coverage/lcov.info | coveralls"
	},
	"directories": {
		"lib": "lib",
		"test": "test"
	},
	"dependencies": {
		"@types/node": "^12.12.7",
		"@types/socket.io-client": "^1.4.32",
		"app-root-path": "^3.0.0",
		"express": "^4.17.1",
		"pointyapi": "^3.3.0",
		"socket.io": "^2.3.0"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^0.1.3",
		"@types/jasmine": "^3.4.6",
		"coveralls": "^3.0.7",
		"jasmine": "^3.5.0",
		"nyc": "^14.1.1",
		"socket.io-client": "^2.3.0",
		"source-map-support": "^0.5.16",
		"ts-node": "^8.5.0",
		"typescript": "^3.7.2"
	},
	"keywords": [],
	"author": "",
	"license": "MIT"
}
