{
	"name": "pointyapi-sockpool",
	"version": "0.0.1",
	"description": "Websocket pool for PointyAPI",
	"main": "index.js",
	"scripts": {
		"test": "npm run build:spec && npm run coverage",
		"build": "tsc --p tsconfig.json",
		"build:spec": "tsc --p tsconfig.json && tsc --p tsconfig.spec.json",
		"postinstall": "tsc --p tsconfig.dist.json",
		"doc": "typedoc --out docs src --excludeNotExported",
		"coverage": "nyc npm run coverage-test",
		"coverage-test": "jasmine --config=test/jasmine.json",
		"coveralls": "cat ./coverage/lcov.info | coveralls"
	},
	"directories": {
		"lib": "lib",
		"test": "test"
	},
	"dependencies": {
		"@types/node": "^10.14.8",
		"@types/socket.io-client": "^1.4.32",
		"app-root-path": "^2.2.1",
		"express": "^4.17.1",
		"pointyapi": "github:StatelessStudio/pointyapi#v2.1.0",
		"socket.io": "^2.2.0"
	},
	"devDependencies": {
		"@types/jasmine": "^3.3.13",
		"source-map-support": "^0.5.12",
		"socket.io-client": "^2.2.0",
		"jasmine": "^3.4.0",
		"nyc": "^14.1.1",
		"coveralls": "^3.0.4",
		"@istanbuljs/nyc-config-typescript": "^0.1.3",
		"istanbul": "^0.4.5"
	},
	"keywords": [],
	"author": "",
	"license": "MIT"
}
